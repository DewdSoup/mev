{
    "name": "@mev/arb-mm",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "main": "dist/src/main.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "//": "──────────── Live reports / utilities ────────────",
        "dev": "tsx watch src/main.ts",
        "build": "tsc -b",
        "typecheck": "tsc --noEmit",
        "backtest": "tsx src/replay.ts",
        "replay": "tsx src/replay.ts",
        "replay:last": "tsx scripts/replay_last.ts",
        "daily": "tsx scripts/daily.ts",
        "daily:if-needed": "tsx scripts/daily_if_needed.ts",
        "report:live": "tsx scripts/live_report.ts",
        "dump:raydium": "tsx scripts/dump_raydium_pool_keys.ts",
        "phoenix:diag": "tsx src/diag.ts",
        "ml:tap": "tsx scripts/ml_tap.ts",
        "start": "node dist/src/main.js",
        "start:prod": "node dist/src/main.js",
        "//_logs": "──────────── Log path unification (no code changes) ────────────",
        "logs:link": "bash -lc 'set -e; REPO_ROOT=$(git rev-parse --show-toplevel 2>/dev/null || pwd); DATA_ROOT=\"$REPO_ROOT/data\"; mkdir -p \"$DATA_ROOT/arb/logs\" \"$DATA_ROOT/amms\" \"$DATA_ROOT/phoenix\"; rm -rf \"$REPO_ROOT/packages/phoenix/logs\" \"$REPO_ROOT/packages/amms/logs\" \"$REPO_ROOT/services/arb-mm/logs\"'",
        "logs:clean": "bash -lc 'set -e; REPO_ROOT=$(git rev-parse --show-toplevel 2>/dev/null || pwd); rm -rf \"$REPO_ROOT/data/arb\" \"$REPO_ROOT/data/amms\" \"$REPO_ROOT/data/phoenix\"'",
        "//_hooks": "──────────── Keep symlinks correct before local runs ────────────",
        "predev": "pnpm run logs:link",
        "prestart": "pnpm run logs:link",
        "prestart:prod": "pnpm run logs:link",
        "live:multi": "tsx src/multipair.ts"
    },
    "dependencies": {
        "@coral-xyz/anchor": "^0.29.0",
        "@ellipsis-labs/phoenix-sdk": "2.0.3",
        "@mev/core": "workspace:*",
        "@mev/phoenix": "workspace:*",
        "@mev/risk": "workspace:*",
        "@mev/storage": "workspace:*",
        "@orca-so/common-sdk": "^0.6.11",
        "@orca-so/whirlpool-sdk": "^0.4.2",
        "@orca-so/whirlpools-sdk": "^0.15.0",
        "@raydium-io/raydium-sdk": "1.3.1-beta.58",
        "@solana/spl-token": "^0.4.13",
        "@solana/web3.js": "^1.98.4",
        "bn.js": "^5.2.2",
        "decimal.js": "^10.6.0",
        "dotenv": "^17.2.1",
        "tslib": "^2.8.1"
    },
    "devDependencies": {
        "tsx": "^4.20.5",
        "typescript": "^5.9.2"
    }
}
